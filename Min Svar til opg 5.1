void setup() {
  size(400, 400);
  background(200);  // Grå baggrund
  
  // Tegn ansigtet (stor cirkel til hovedet)
  fill(255);  // Hvid farve
  ellipse(200, 250, 200, 200);  // x, y, bredde, højde
  
  // Tegn øjnene (rektangler)
  fill(255);  // Hvid farve
  rect(150, 220, 40, 40);  // Venstre øje
  rect(210, 220, 40, 40);  // Højre øje
  
  // Tegn næsen (lille cirkel)
  fill(255);  // Hvid farve
  ellipse(200, 270, 40, 40);  // Næse
  
  // Tegn munden (bue)
  noFill();
  arc(200, 310, 80, 40, 0, PI);  // x, y, bredde, højde, startvinkel, slutvinkel
  
  // Tegn hatten (rektangler)
  fill(0);  // Sort farve
  rect(150, 50, 100, 150);  // Hoveddel af hatten
  rect(130, 200, 140, 10);  // Rand af hatten
}
